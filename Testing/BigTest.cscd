~CSCD?U~

;; Top-level object demonstrating all CSCD literals and metadata ;;

`rootObj`(my.namespace.RootType<int>)<
    
    ;; Null and booleans ;;
    nullValue: (nullable_type)null,
    boolTrue: true,
    boolFalse: false,
    
    ;; Integers ;;
    intZero: 0,
    intNegative: -00042,
    
    ;; Floats ;;
    floatFull: 1.2300e02,
    floatFull2: 1.2300e-02,
    floatFull3: -1.2300e02,
    floatFull4: -1.2300e-02,
    floatNoExp: -5.5,
    floatIntOnly: 10.,
    floatFracOnly: -.0103,
    floatFracOnly: .080e-5,
    floatIntExp: 7e-500,
    floatJustDot: .,
    floatNegZero: -.,
    
    ;; Infinity and NaN ;;
    posInf: inf,
    negInf: -inf,
    notANumber: nan,
    
    ;; Characters ;;
    charA: 'A',
    charQuote: ''',
    charEscaped: '\n',
    charQuoteEscaped: '\'',
    charUnicode: '\21FF;',
    charEmpty: '',
    
    ;; Strings ;;
    simpleString: "Hello, World!",
    escapedString: "Line1\r\nLine2\tTabbed \"quote\" \\backslash\\",
    unicodeString: "Arrow: \21FF; and Euro: \20AC;",
    emptyString: "",
    
    ;; Decimals ;;
    decimalFull: $123.4500,
    decimalNoInt: $.75,
    decimalIntegral: $100,
    decimalOnlySign: -$,
    
    ;; Colors ;;
    colorFull: #FFAABBCC,
    colorNoAlpha: #112233,
    colorShortAlpha: #8F0A,
    colorShort: #ABC,
    colorEmpty: #,
    
    ;; UIDs ;;
    uidFull: %69988773-1484-832f-9fe1-a711cf10115f,
    uidNoDashes: %6998bd06ed3083338d8f142c0f7e52f5,
    uidShort: %111,
    uidEmpty: %,

    ;; Timestamps and offsets ;;
    timestampFull: |+2:30| @2020/12/31,23:59:60.500@,
    timestampNoSecond: @2020/12/31,23:59@,
    timestampNoMinuteSecond: @2020/12/31,23@,
    timestampDateOnly: @-500/2/7@,
    timestampDateOnlyNoDay: @-500/2@,
    timestampDateOnlyNoMonthDay: @-500@,
    timestampTimeOnly: @07:30:00@,
    timestampTimeOnlyNoSecond: |Z| @01:30@,
    timestampEmpty: @@,
    
    ;; Durations ;;
    durationFull: 5d10h59m30.5s,
    durationNoMonths: 800d0m.s,
    durationNoDaysAndSeconds: 1h30m,
    durationNoDaysAndMinutes: 5h5.0e-5s,
    durationNegative: -30s,
    durationZero: 0s,
    durationExpSeconds: 50m.1e-5s,
    
    ;; Bytes ;;
    bytesFull: !AAECAwQFBgc=,
    bytesNoPadding: !AAECAwQFBgc,
    bytesEmpty: !,
    
    ;; Symbols ;;
    bareSymbol: MyEnumValue,
    delimitedSymbol: *\*Complex Symbol Name!*,
    emptySymbol: **,
    reservedBareAsDelimited: *null*,
    
    ;; Shared references ;;
    sharedList: `shared_list`[1,2,3],
    refToShared: &shared_list&,
    
    ;; Dictionary ;;
    sampleDict: {
        "key1": 1,
        'k': "value",
        3.14: "pi",
        *symKey*: $9.99
    },
    
    ;; Nested object with scopes and duplicate names ;;
    nestedObject: `nested\`1`(custom.Type)<
        ^complicated\^base\^class^ ;; comment;; value: ;; comment2 ;; &shared_list&,
        ^base^value: 1,
        value: 2,
        listInside: [
            &shared_list&,
            (\(typedItem\))"abc",
            `innerId`42,
            &innerId&
        ]
    >,
    
    ;; List with mixed metadata ;;
    mixedList: [
        null,
        (i32)5,
        `idInList&`"text",
        &idInList\&&,
        <member: $.01>,
        { true : false }
    ],

    ;; Parser comment torture. ;;
    torture_l1: [;;e;;],
    torture_l2: [;;e;;0;;e;;],
    torture_l3: [;;e;;0;;e;;,;;e;;1;;e;;],
    torture_d1: {;;e;;},
    torture_d2: {;;e;;0;;e;;:;;e;;1;;e;;},
    torture_d3: {;;e;;0;;e;;:;;e;;1;;e;;,;;e;;'A';;e;;:;;e;;'B';;e;;},
    torture_o1: <;;e;;>,
    torture_o2: <;;e;;a;;e;;:;;e;;0;;e;;>,
    torture_o3: <;;e;;a;;e;;:;;e;;0;;e;;,;;e;;b;;e;;:;;e;;0;;e;;>,
    torture_o4: <;;e;;a;;e;;:;;e;;0;;e;;,;;e;;^base^;;e;;a;;e;;:;;e;;0;;e;;>,
    torture_meta: `a`;;e;;(;;e;;b;;e;;);;e;;|Z|;;e;;@@
>

~/CSCD~