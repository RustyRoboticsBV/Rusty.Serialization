{
  "name": "CSCD Default Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E23",
    "editor.foreground": "#E6E6E6",
    "editorBracketHighlight.foreground1": "#BDBD82",
    "editorBracketHighlight.foreground2": "#BDBD82",
    "editorBracketHighlight.foreground3": "#e8eee5",
    "editorBracketHighlight.foreground4": "#e8eee5",
    "editorBracketHighlight.foreground5": "#e8eee5",
    "editorBracketHighlight.foreground6": "#e8eee5",
    "editorBracketHighlight.unexpectedBracket.foreground": "#F44747"
  },
  "editor.bracketPairColorization.enabled": false,
  "tokenColors": [

    // =============================
    // FORMAT MARKERS
    // =============================
    {
      "scope": "cscd.markers",
      "settings": { "foreground": "#535353", "fontStyle": "bold italic" }
    },

    // =============================
    // COMMENTS
    // =============================
    {
      "scope": "cscd.comment",
      "settings": { "foreground": "#6A9955", "fontStyle": "italic" }
    },

    // =============================
    // PUNCTUATION
    // =============================
    {
      "scope": "cscd.punctuation",
      "settings": { "foreground": "#e8eee5", "fontStyle": "bold" }
    },

    // =============================
    // ESCAPE SEQUENCES
    // =============================
    {
      "scope": "cscd.escapes.string",
      "settings": { "foreground": "#CFBC78", "fontStyle": "bold" }
    },
    {
      "scope": "cscd.escapes.character",
      "settings": { "foreground": "#D7BA7D", "fontStyle": "bold" }
    },

    // =============================
    // METADATA
    // =============================
    {
      "scope": "cscd.metadata.address",
      "settings": { "foreground": "#7c7cd9", "fontStyle": "bold" }
    },
    {
      "scope": "cscd.metadata.type",
      "settings": { "foreground": "#BDBD82", "fontStyle": "bold" }
    },
    {
      "scope": "cscd.metadata.scope",
      "settings": { "foreground": "#dcaab2", "fontStyle": "bold" }
    },
    {
      "scope": "cscd.metadata.offset",
      "settings": { "foreground": "#BBF547", "fontStyle": "bold" }
    },

    // =============================
    // PRIMITIVES
    // =============================
    {
      "scope": "cscd.primitives.null",
      "settings": { "foreground": "#888888", "fontStyle": "bold" }
    },
    
    {
      "scope": "cscd.primitives.boolean",
      "settings": { "foreground": "#00CED1", "fontStyle": "bold" }
    },
    
    {
      "scope": "cscd.primitives.integer",
      "settings": { "foreground": "#569CD6", "fontStyle": "bold" }
    },

    {
      "scope": "cscd.primitives.float",
      "settings": { "foreground": "#F44747", "fontStyle": "bold" }
    },

    {
      "scope": "cscd.primitives.infinity",
      "settings": { "foreground": "#FF00FF", "fontStyle": "bold" }
    },

    {
      "scope": "cscd.primitives.nan",
      "settings": { "foreground": "#FF8C00", "fontStyle": "bold" }
    },
    
    {
      "scope": "cscd.primitives.character",
      "settings": { "foreground": "#D7BA7D", "fontStyle": "bold" }
    },

    {
      "scope": "cscd.primitives.string",
      "settings": { "foreground": "#CE9178", "fontStyle": "bold" }
    },
    
    {
      "scope": "cscd.primitives.decimal",
      "settings": { "foreground": "#4EC9B0", "fontStyle": "bold" }
    },
    
    {
      "scope": "cscd.primitives.color",
      "settings": { "foreground": "#FF4081", "fontStyle": "bold" }
    },

    {
      "scope": "cscd.primitives.uid",
      "settings": { "foreground": "#a000d1", "fontStyle": "bold" }
    },

    {
      "scope": "cscd.primitives.timestamp",
      "settings": { "foreground": "#FFBF00", "fontStyle": "bold" }
    },

    {
      "scope": "cscd.primitives.duration",
      "settings": { "foreground": "#80FF80", "fontStyle": "bold" }
    },

    {
      "scope": "cscd.primitives.bytes",
      "settings": { "foreground": "#7720F0", "fontStyle": "bold" }
    },

    {
      "scope": "cscd.primitives.reference",
      "settings": { "foreground": "#FFFF00", "fontStyle": "bold" }
    },

    {
      "scope": "cscd.primitives.symbol",
      "settings": { "foreground": "#FFFFFF" }
    },

    // =============================
    // COLLECTIONS
    // =============================
    {
      "scope": "cscd.collections.list",
      "settings": { "foreground": "#D4D4D4" }
    },

    {
      "scope": "cscd.collections.dictionary",
      "settings": { "foreground": "#D4D4D4" }
    },

    {
      "scope": "cscd.collections.object",
      "settings": { "foreground": "#D4D4D4" }
    },

    // =============================
    // DELIMITERS
    // =============================
    {
      "scope": [
        "punctuation.separator.comma.cscd",
        "punctuation.separator.colon.cscd"
      ],
      "settings": { "foreground": "#808080" }
    }
  ]
}